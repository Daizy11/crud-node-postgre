    <div class="content-container">
      <div class="table-wrapper">
        <div class="ml-3 p-2 flex justify-between">
          <h3 style="font-size: 2rem">Produk</h3>
          <a href="add">
            <button
              style="padding: 7px; margin: 0px 0px 0px 20px; width: 5rem"
              class="btn-add"
            >
              ADD
            </button></a
          >
        </div>

        <table class="table">
          <thead>
            <tr>
              <th>No</th>
              <th>Nama Produk</th>
              <th>Produk CategoryID</th>
              <th style="width: 200px">Actions</th>
            </tr>
          </thead>

          <tbody id="tbody">
            <% users.data.forEach((row,index)=>{ %>
            <tr>
              <td><%= (page - 1) * limit + index + 1 %></td>
              <td><%= row.product %></td>
              <td><%= row.productCategoryID %></td>
              <td class="action">
                <a href="/edit/<%= row.id %>">
                  <button class="btn-edit">Edit</button></a
                >
                <a href="/delete/<%= row.id %>">
                  <button class="btn-delete">Delete</button>
                </a>
                <!-- <button class="btn-delete">Delete</button> -->
              </td>
            </tr>

            <% }) %>
          </tbody>
        </table>

        <div
          class="flex items-center justify-between border-t border-gray-200 bg-white px-4 py-3 sm:px-6 "
        >
